<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.tiktokcoinscalculator">

    <application
        android:name=".TiktokCoinsCalcApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launch_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launch_app_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3161809630065444~8794749891     "/>
        <activity
            android:name=".LaunchActivity" android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>